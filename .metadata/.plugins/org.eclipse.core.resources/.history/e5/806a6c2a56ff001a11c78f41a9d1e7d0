<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ProjectRepository">
  
  
  
  <!-- 파입 업로드 다운로드 처리 -->
  <insert id="insertFile" parameterType="File">
  	insert into file
  	(
  		f_no,
  		g_no,
  		org_name,
  		sav_name,
  		file_size,
  		f_regdate,
  	)
  	values
  	(
  		f_no.nextval,
  		g_no.currval,
  		#{org_name},
  		#{sav_name},
  		#{file_size},
  		sysdate
  	)
  
  </insert>
  
  
  
</mapper>  