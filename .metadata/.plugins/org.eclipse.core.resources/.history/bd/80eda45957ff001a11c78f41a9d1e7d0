package com.spring.withwork.dao;

import org.mybatis.spring.SqlSessionTemplate;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.spring.withwork.vo.File;

@Repository("projectRepository")
public class ProjectRepository {

	
	@Autowired
	SqlSessionTemplate mybatis;
	
	public ProjectRepository() {
		System.out.println(">> ProjectRepository 생성자 생성");
	}
	
	
	
	
	
//	@param 파일 업로드 다운로드 관련 처리.
	public int insertFile(File vo){
		int result = 0; 
		try{
			result = mybatis.insert("ProjectRepository.insertFile", vo);
		}catch(Exception e){
			e.printStackTrace();
		}
		return result; 
	}
	
	
	
	public File selectFile(String f_no){
		File result = null; 
		try{
			result = mybatis.select("ProjectRepository.selectFile", f_no);
			
		}catch(Exception e){
			e.printStackTrace();
		}
		return result; 
		
	}
	public File selectFileBySeq(String f_no);
	
	
}
